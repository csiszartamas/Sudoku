<Window x:Class="Sudoku.Sudoku_game"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sudoku"
        mc:Ignorable="d"
        Title="Sudoku_game" Height="450" Width="800">
    <Grid Loaded="Grid_Loaded">
        <Button x:Name="BT_ujjatek" Content="Új Játék" HorizontalAlignment="Left" Height="44" Width="110"  Margin="508,30,0,0" VerticalAlignment="Top" Click="BT_ujjatek_Click"/>
        <Button x:Name="BT_ranglista" Content="Ranglista" HorizontalAlignment="Left" Height="44" Width="110"  Margin="508,140,0,0" VerticalAlignment="Top" Click="BT_ranglista_Click"/>
        <Button x:Name="BT_beallitasok" Content="Beállítások" HorizontalAlignment="Left" Height="44" Width="110"  Margin="508,200,0,0" VerticalAlignment="Top" Click="BT_beallitasok_Click"/>
        <Label Content="Idő:" Margin="462,72,0,0"   HorizontalAlignment="Left" VerticalAlignment="Top" Width="46"></Label>

        <TextBox x:Name="TB_ido" HorizontalAlignment="Left" Margin="509,77,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="109"/>
        <Label x:Name="LB_koszontes" Content="Label" HorizontalAlignment="Left" Margin="546,270,0,0" VerticalAlignment="Top" Height="121" Width="215"/>

    </Grid>
</Window>
